<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" 
    xmlns="http://java.sun.com/xml/ns/j2ee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
      http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <servlet>
    <servlet-name>Servletjsp</servlet-name>
    <servlet-class>ServletToJSP.ServletToJSP</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servletjsp</servlet-name>
    <url-pattern>/ServletToJSP</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>ServletSearch</servlet-name>
    <servlet-class>ServletToJSP.ServletSearch</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletSearch</servlet-name>
    <url-pattern>/ServletSearch</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Servletcheckout</servlet-name>
    <servlet-class>ServletToCheckout.ServletToCheckout</servlet-class>
    <load-on-startup>3</load-on-startup>
</servlet>
  <servlet-mapping>
    <servlet-name>Servletcheckout</servlet-name>
    <url-pattern>/ServletToCheckout</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>checkout.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>Servletadd</servlet-name>
    <servlet-class>ServletToAdd.ServletToAdd</servlet-class>
    <load-on-startup>4</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servletadd</servlet-name>
    <url-pattern>/ServletToAdd</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>Servletpdf</servlet-name>
    <servlet-class>PDFServlet.PDFServlet</servlet-class>
    <load-on-startup>5</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servletpdf</servlet-name>
    <url-pattern>/PDFServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>checkout.jsp</welcome-file>
  </welcome-file-list>
</web-app>
